#include<stdio.h>
int main(int argc, char **argv) {
    printf("%d\n",argc);
    for(int i=0;i<argc;i++)
    {
        printf("arg[%d] - %s\n",i,argv[i]);
    }
   

    return 0;
}



cmd line:  ./main.out hello how are you
5
arg[0] - ./main.out
arg[1] - hello
arg[2] - how
arg[3] - are
arg[4] - you




Eg2:
#include<stdio.h>
int main(int argc, char **argv) {
    printf("%d\n",argc);
    for(int i=0;i<argc;i++)
    {
        printf("arg[%d] - %s\n",i,argv[i]);
    }
   
    return 0;
}

cmdlineargs:  ./main.out "Hello how r u"
o/p:
2
arg[0] - ./main.out
arg[1] - Hello how r u
